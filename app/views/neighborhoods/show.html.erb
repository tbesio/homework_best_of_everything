<div class="page-header">
  <h1>Neighborhood #<%= @neighborhood.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>Name</dt>
  <dd><%= @neighborhood.name %></dd>

  <dt>City</dt>
  <dd><%= @neighborhood.city %></dd>

  <dt>Venues</dt>
  <% @venues.each do |venue| %>
    <dd><%= venue.name %></dd>
  <% end %>

</dl>
